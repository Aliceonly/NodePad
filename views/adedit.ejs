<%- include header %>
<form method="post">

	<div class="form-group">
		<input autofocus type="text" required class="form-control" name="username" placeholder="用户名" value="<%= datas.username %>">
	</div>

	<div class="form-group">
		<input type="text" class="form-control" required name="email" placeholder="邮箱" value="<%= datas.email %>">
	</div>

	<button id="enter" type="submit" style="outline: none;" class="btn btn-default">保存</button>

</form>
<%- include footer %>